<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="VSL.Versions.targets"/>
  <Import Project="Settings.targets"/>
  <!-- We build on ProjectN only C# code currently: -->
  <PropertyGroup Condition="'$(BuildingWithProjectN)' == 'true' AND '$(ProjectLanguage)' == 'CSharp'">
    <DefineConstants>$(DefineConstants);ON_PROJECTN</DefineConstants>
  </PropertyGroup>
  <!-- Prepare code for x86 compilation: -->
  <PropertyGroup Condition="'$(BuildingWithProjectN32)' == 'true' AND '$(ProjectLanguage)' == 'CSharp'">
    <DefineConstants>$(DefineConstants);ON_PROJECTN;ON_PROJECTN32</DefineConstants>
  </PropertyGroup>

</Project>
